/* pages/fieldDetail/fieldDetail.wxss */
.item-img {
  padding: 20rpx 30rpx;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
}
.item-tit-wrap {
  overflow: hidden;
  padding: 10rpx 30rpx;
  width: 690rpx;
  display: flex;
  background-color: #fff;
}
.item-box{
  padding:10rpx 0rpx 20rpx;
  background-color: #fff;
}
.item-tit {
  color: #333;
  font-size: 34rpx;
  font-weight: 700;
  line-height: 50rpx;
  flex: 1;
}
.item-follow {
  padding-left: 50rpx;
  font-size: 34rpx;
  margin-left: 20rpx;
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-position: left center;
  background-size: 45rpx 45rpx;
  line-height: 50rpx;
  height: 50rpx;
}
.follow {
  background-image: url('data:image/png;base64,');
}

.item-row {
  display: flex;
  padding: 10rpx 30rpx;
  background-color: #fff;
  font-size: 30rpx;
  color: #999;
}
.item-key {
  width: 25%;
}
.item-val {
   width: 75%;
   word-break:break-all;
   word-wrap:break-word;
  /* display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis; */
}
.two_line {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
}
.look-daile {
  text-align: right;
  background-color: #fff;
  color: #2b7dd9;
  padding-right: 30rpx;
  font-size: 30rpx;
  padding-bottom: 30rpx;
}

/* swich-tab */
.swich-tab {
  margin-top: 20rpx;
  background-color: #fff;
  text-align: center;
  display: flex;
}
.swich-tab text {
  font-size: 28rpx;
  color: #999;
  line-height: 80rpx;
  flex: 1;
  border-bottom: 1rpx solid #ddd;
  transition: all .2s;
}
.log-tab {
  margin: 20rpx 30rpx;
  border-radius: 10rpx;
  overflow: hidden;
}
.log-tab text {border: none;}
.swich-tab .on {
  color: #f60;
  font-size: 32rpx;
  border-color: #f60;
}
.log-tab .on {
  color: #fff;
  background-color: #f60;
}
/* 地理位置-css-start */
.model-map {
  padding: 20rpx 30rpx;
  background-color: #fff;
}
.region-name {
  font-size: 32rpx;
  text-align: center;
  color: #666;
  margin-bottom: 20rpx;
}
.field-map {
  width: 100%;
  height: 690rpx;
}

/* 示范日志-css-start */
.farm-log {
  background-color: #f5f5f5;
  /* padding: 30rpx; */
}
.field,.log-desc {
  padding: 30rpx;
  background-color: #fff;
  margin-bottom: 30rpx;
}
.farm-tit {
  /* padding: 20rpx 30rpx 0; */
  margin: 20rpx 0;
  background-color: #fff;
  font-size: 34rpx;
  overflow: hidden;
}
.farm-tit text:nth-child(2) {float: right;}
.log-row {
  border: 1rpx solid #ddd;
  /* border-bottom: none; */
  display: flex;
  font-size: 30rpx;
  line-height: 50rpx;
  color: #666;
}
.log-row:last-child {
  border-bottom: 1px solid #ddd;
}
.log-key {
  padding: 10rpx 15rpx;
  border-right: 1rpx solid #ddd;
  width: 180rpx;
}
.log-val {padding: 10rpx 15rpx;}
.log-desc {
  color: #666;
  font-size: 30rpx;
  line-height: 1.5;
  /* text-indent: 2em; */
}
.log-imgs {
  border-radius: 10rpx;
  display: flex;
  flex-wrap: wrap;
  align-items:center;
  padding: 30rpx 1rpx;
}
 .log-imgs image {
  width: 33.33%;
  box-shadow: 0 0 1rpx #9b9b9b;
} 
/* 点赞评论-css-start */
.logs-other {
  overflow: hidden;
  margin-bottom: 30rpx;
  margin-top: 20rpx;
}
.log-desc .logs-other .praise {
  float: right;
  color: #390;
  padding-left: 50rpx; 
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-position: left center;
  background-size:40rpx 40rpx;
  margin-left: 30rpx;
  font-size: 28rpx;
}
.log-desc .logs-other .comment {
  float: right;
  color: #390;
  padding-left: 50rpx; 
  background-repeat: no-repeat;
  background-position: left center;
  background-size:40rpx 40rpx;
  margin-left: 30rpx;
  font-size: 28rpx;
  background-image: url('data:image/png;base64,');
}
.field .logs-other .praise,.field .logs-other .comment {
  float: right;
  color: #390;
  padding-left: 50rpx; 
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-position: left center;
  background-size:40rpx 40rpx;
  margin-left: 30rpx;
  font-size: 28rpx;
}
.field .logs-other .comment {background-image: url('data:image/png;base64,');}

/* 用户留言-start */
.msges-wrap {
  padding: 30rpx;
  background-color: #fff;
  overflow: hidden;
}
.leave-word {
   border: 1rpx solid #ddd; 
  border-radius: 10rpx;
  padding: 10rpx;
  margin-bottom: 30rpx;
  overflow: hidden;
}
.leave-word textarea {
  height: 160rpx;
  width: 100%;
  font-size: 30rpx;
  color: #353535;
  margin-bottom: 15rpx;
}
.leave-word text {
  color: #999;
  font-size: 26rpx;
  line-height: 50rpx;
}
.leave-word button {
  padding: 0;
  margin: 0;
  float: right;
  width: 120rpx;
  line-height: 60rpx;
  height: 60rpx;
  font-size: 30rpx;
  background-color: #1AAD19;
}


.msges-list {
  overflow: hidden;
  /* border-bottom: 1rpx dashed #ddd; */
}
.msg-wrap {  
  padding: 0 90rpx;
  overflow: hidden;
  margin-bottom: 20rpx;
}
.head-img {
  width: 60rpx;
  height: 60rpx;
  margin-left: -90rpx;
  float: left;
  border:1rpx solid #ddd;
  border-radius: 50%;
  padding: 5rpx;
}
.head-img image {
  width: 60rpx;
  height: 60rpx;
  float: left;
  border-radius: 50%;
}
.msg-info {
  width: 550rpx;
  float: left;
  background-color: #eee;
  color: #666;
  padding: 20rpx;
  border-radius: 10rpx;
  position: relative;
}
.msg-info::before {
  content: "";
  position: absolute;
  left: -13rpx;
  top: 20rpx;
  width: 0;
  height: 0;
  border-bottom: 15rpx solid transparent;
  border-top: 15rpx solid transparent;
  border-right: 15rpx solid #eee;
}
.name {
  font-size: 28rpx;
}
.msg {
  font-size: 26rpx;
}
.msg-res {float: right;}
.msg-res .msg-info {
  float: right;
  background-color: #f60;
}
.msg-res .head-img {
  float: right;
  margin-left: 0;
  margin-right: -90rpx;
}
.msg-res .msg-info::before {
  left: auto;
  right: -13rpx;
  border-right: none;
  border-left: 15rpx solid #f60;
}
.msg {
  word-break:break-all;
  word-wrap:break-word;
  padding-top: 18rpx;
  font-size: 28rpx;
}
/* 发表评论 */
.textarea-wrap {
  border-radius: 10rpx;
  height: 75rpx;
  border: 1rpx solid #ddd;
  display: flex;
  overflow: hidden;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
.textarea-wrap input {
  flex: 1;
  font-size: 30rpx;
  color: #333;
  padding:0 15rpx;
  height: 75rpx;
}
.textarea-wrap button {
  padding: 0;
  margin: 0;
  width: 160rpx;
  height: 75rpx;
  line-height: 75rpx;
  border-radius: 0;
  font-size: 32rpx;
}
.textarea-wrap button::after {content: none;}
.hide {
  display: none;
}
.show {
  display: block;
}
.prompt {
  text-align: center;
  color: #999;
  font-size: 32rpx;
  padding: 20rpx 0;
  
}
.bus-info {
  font-size: 28rpx;
  margin-top: 30rpx;
  color: #f93;
}
.bus-info view {
  word-break:break-all;
  word-wrap:break-word;
}
.log-img {
  height: 180rpx;
}
.textarea-wrap textarea {
  flex: 1;
  font-size: 30rpx;
  color: #333;
  padding:10rpx 15rpx;
  height: 75rpx;
  
}