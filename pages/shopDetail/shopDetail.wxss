/* 评价 */
.zm_star {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 20rpx;
  background: #ffffff;
}
.zm_star .zm_star_top {
  width: 700rpx;
  padding: 0 25rpx;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  border-bottom: 1rpx solid #eeeeee;
}
.zm_star .zm_star_top .zm_star_top_title {
  height: 80rpx;
  display: flex;
  /* justify-content: center; */
  align-items: center;
  color:#333333;
  font-size:28rpx;
}
.zm_star .zm_star_top .zm_star_top_num {
  height: 80rpx;
  display: flex;
  /* justify-content: center; */
  align-items: center;
  color:#aaaaaa;
  font-size:26rpx;
}

.zm_star_bottom {
  width: 700rpx;
}
.zm_star_bottom .star_list {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.star_list .star_item {
  width: 670rpx;
  padding: 25rpx 20rpx;
  border-bottom: 1rpx solid #eeeeee;
}
.star_item .star_top {
  width: 100%;
  height: 30rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
}
.star_top .star_top_left {
  height: 30rpx;
  display: flex;
  flex-direction: row;
}
.star_top .star_top_left .star_tel {
  height: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	color: #333333;
}
.star_top .star_top_left .star_star {
  width: 195rpx;
  height: 30rpx;
  margin-left: 33rpx;
  display: flex;
  flex-direction: row;
  /* justify-content: space-between; */
}
.star_top .star_top_left .star_star .star_icon {
  width: 30rpx;
  height: 30rpx;
  margin-right: 8rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.star_top .star_top_left .star_star .star_icon .star_icon_img {
  width: 30rpx;
  height: 30rpx;
}
.star_top .star_time {
  height: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	color: #aaaaaa;
}
.star_item .star_container {
  width: 100%;
  overflow: hidden;
  /* margin-top: 38rpx; */
  margin-bottom: 22rpx;
}
.star_item .star_container .star_container_text {
  width: 650rpx;
  font-size: 28rpx;
  text-align: left;
  color: #333333;
  line-height: 35rpx;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:4;
  overflow: hidden;
  text-overflow:ellipsis;
}
.star_item .star_update {
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  /* display: none; */
}
.star_item .star_update .star_update_img {
  width: 150rpx;
  height: 150rpx;
  margin-right: 23rpx;
  /* margin-bottom: 20rpx; */
}
.star_item .star_update .star_update_img:nth-child(4) {
    margin-right: 0rpx;
}
.star_item .star_update .star_update_img:nth-child(8) {
  margin-right: 0rpx;
}
.star_item .star_norm {
  width: 100%;
  font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	color: #aaaaaa;
}
.star_item .star_reply {
  width: 610rpx;
  padding: 23rpx 44rpx 33rpx 26rpx;
  margin-top: 20rpx;
  background: #f5f5f5;
  border-radius: 3rpx;
  display: flex;
  flex-direction: column;
}
.star_item .star_reply .star_reply_top {
  width: 100%;
  height: 50rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.star_item .star_reply .star_reply_top .star_reply_icon {
  width: 50rpx;
  height: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.star_item .star_reply .star_reply_top .star_reply_icon .star_reply_img {
  width: 28rpx;
  height: 28rpx;
}
.star_item .star_reply .star_reply_top .star_reply_title {
  height: 50rpx;
  font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
  color: #666666;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 10rpx;
}
.star_item .star_reply .star_reply_content {
  width: 100%;
  margin-top: 20rpx;
  font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 42rpx;
	letter-spacing: 2rpx;
	color: #333333;
}
.zm_star_last {
  width: 100%;
