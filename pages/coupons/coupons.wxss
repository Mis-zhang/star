/*优惠劵的选择*/

.orderdetail {
  width: 100%;
  height: auto;
}

page {
  background: #fff;
}

.coupon-choose {
  width: 100%;
  height: 200rpx;
  margin: 20rpx 0rpx;
  background-color: #fff;
}

.coupons-con {
  width: 100%;
  float: left;
  height: 100%;
}

.block {
  height: 100%;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
}

.coupons {
  width: 620rpx;
  height: 210rpx;
  background: url('data:image/png;base64,') no-repeat center center;
  background-size: 620rpx 210rpx;
  text-align: left;
  color: #e4383b;
  float: left;
  overflow: hidden;
  margin-left: 85rpx;
  position: relative;
  margin-bottom: 30rpx;
}

.invalid {
  width: 600rpx;
  height: 100rpx;
  background: #ccc;
  margin-left: 85rpx;
  margin-top: 30rpx;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  font-size: 35rpx;
  margin-bottom: 20rpx;
  font-weight: 600;
  display: none;
}

.discount {
  width: 280rpx;
  height: 100%;
  float: left;
  text-align: center;
}

.discount text {
  font-size: 36rpx;
  font-weight: 700;
  color: #fff;
}

.discount .quota {
  font-size: 120rpx;
  font-weight: 700;
  color: #fff;
  line-height: 200rpx;
}

.draw {
  width: 100rpx;
  height: auto;
  text-align: center;
  color: #fff;
  font-size: 35rpx;
  float: left;
  font-weight: 600;
}

.draw1 {
  width: 100rpx;
  height: auto;
  text-align: center;
  color: #fff;
  font-size: 35rpx;
  float: left;
  font-weight: 600;
}

.draw1 text {
  display: block;
  width: 60rpx;
  height: 100%;
  margin: 0 auto;
  margin-top: 25rpx;
  text-align: right;
  margin-left:18rpx;
}

.draw text {
  display: block;
  width: 60rpx;
  height: 100%;
  margin: 0 auto;
  margin-top: 25rpx;
  text-align: right;
  margin-left:18rpx;
}

.To {
  display: block;
  width: 25rpx;
  height: 25rpx;
  text-align: center;
  line-height: 20rpx;
  font-size: 25rpx;
  color: #fff;
  margin-left: 175rpx;
}

.term {
  width: 235rpx;
  height: 200rpx;
  float: left;
  text-align: left;
}

.term .date {
  font-size: 30rpx;
  color: #fff;
  display: block;
  width: 100%;
  height: 66rpx;
  line-height: 66rpx;
}

.term .full {
  font-size: 30rpx;
  color: #e50338;
  display: block;
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  background: #fff362;
  font-weight: 600;
  text-align: center;
  margin-top: 10rpx;
}

.term .time {
  font-size: 30rpx;
  color: #fff;
  display: block;
  width: 100%;
  font-weight: 600;
  line-height: 18rpx;
}

.coupons .requirement {
  display: block;
  font-size: 22rpx;
  color: #e05d32;
}

.order-tabBar {
  display: flex;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  color: #333;
  font-size: 30rpx;
  background: #fff;
  margin-bottom: 30rpx;
}

.order-tabBar text {
  flex: 3;
  border-bottom: 1px solid #f5f5f5;
  text-align: center;
}

.order-tabBar .red {
  border-bottom: 1px solid #e4383b;
  color: #e4383b;
}

.show {
  display: block;
}

.hidden {
  display: none;
}
