/* pages/orderConfirm/orderConfirm.wxss */

/* 收货地址添加*/

.addr-opt {
  padding: 36rpx 100rpx;
  font-size: 28rpx;
  color: #353535;
  background: url(data:image/png;base64,) no-repeat 30rpx center,
                url(data:image/png;base64,) no-repeat 680rpx center;
  background-color: #fff;
  margin-top: 20rpx;
  background-size: 45rpx 51rpx;
}

.addr-opt text {
  display: block;
  color: #353535;
  font-size: 26rpx;
  line-height: 1;
}

.addr-opt .consignee {
  font-size: 32rpx;
}

.shopqty {
  padding-left: 30rpx;
}

/*订单详情*/
.gray {
  filter: grayscale(100%);
}
.show {
  display: inherit;
}
.hide {
  display: none;
}
.shop-detail {
  width: 100%;
  margin-top: 20rpx;
  background-color: #fff;
  padding: 30rpx 0 30rpx 30rpx;
  position: relative;
  overflow: hidden;
  box-sizing: border-box;
  display: flex;
}

.shop-detail image {
  float: left;
  width: 170rpx;
  height: 170rpx;
  /* border-bottom: 1px solid #fef5f5; */
}

.shop-detail .company-name {
  display: block;
  font-size: 30rpx;
  color: #c3c3c3;
  line-height: 1;
  overflow: hidden;
  margin-top: 10rpx;
  height: 61rpx;
}

.info-wrap {
  padding: 0 30rpx;
  width: 380rpx;
  overflow: hidden;
  flex: 1;
}

.info-wrap .tit {
  font-size: 30rpx;
  color: #000;
  overflow: hidden;
  line-height: 40rpx;
}

.price-info {
  padding: 10rpx 0;
  overflow: hidden;
}

.price-info text {
  font-size: 26rpx;
  color: #b2b2b2;
  float: left;
}

.price-info .price {
  font-size: 35rpx;
  color: #e4383b;
  float: right;
}


.dsnone{
  display: none;
}
/*商品的总额和运费*/

.coast {
  width: 100%;
  height: auto;
  margin-top: 20rpx;
  color: #b2b2b2;
  background: #fff;
  display: flex;
  flex-direction: column;
}

.coast .total-fee, .freight, .full-cut, .full-folding, .integral {
  flex: 1;
  margin: 15rpx 35rpx;
  font-size: 30rpx;
}

.coast .shop-coast, .freight-txt, .cut-txt, .folding-txt, .integral-txt {
  float: left;
}

.coast .shop-fee, .freight-fee, .cut-fee, .folding-fee, .integral-fee {
  float: right;
}

.coast .strickout{
  text-decoration: line-through;
  color: #b2b2b2;
}
/*商品满赠的物品的名称，规格，和数量*/

.full-gifts {
  width: 100%;
  height: auto;
  margin-top: 20rpx;
  background: #fff;
  color: #b2b2b2;
  font-size: 30rpx;
  text-align: right;
  line-height: 100rpx;
}

.giftdetail {
  width: 650rpx;
  height: auto;
  display: flex;
  flex-direction: column;
  background-color: #fff;
  margin-left: 100rpx;
}

.zeng {
  padding-left: 35rpx;
  float: left;
}

.giftData {
  border-bottom: 1px solid #f5f5f5;
  flex: 1;
}

.giftname {
  float: left;
}

.giftnumber {
  float: right;
  padding-right: 40rpx;
}

/*优惠劵的选择*/

.orderdetail {
  width: 100%;
  height: auto;
}

.coupon-choose {
  width: 100%;
  height: 100rpx;
  margin: 20rpx 0rpx;
  line-height: 100rpx;
  background-color: #fff;
}

.coupon-choose .choice {
  color: #949494;
  font-size: 30rpx;
  width: 180rpx;
  margin-left: 4.66%;
  float: left;
  height: 100%;
}

.coupons-con {
  width: 455rpx;
  line-height: 100rpx;
  float: right;
  height: 100%;
}

.couponNone {
  width: 520rpx;
  line-height: 100rpx;
  float: left;
  height: 100%;
  text-align: right;
  font-size: 30rpx;
  color: #b2b2b2;
}

.block {
  height: 100%;
  position: absolute;
  top: 0rpx;
  left: 10rpx;
}

.coupons {
  width: 150rpx;
  height: 100rpx;
  margin-right: 14rpx;
  /* background: url('data:image/png;base64,') no-repeat center center; */
  background-size: 110rpx;
  box-sizing: border-box;
  text-align: center;
  line-height: 100rpx;
  color: #e4383b;
  float: right;
  overflow: hidden;
}

.coupons text {
  font-size: 25rpx;
  font-weight: 700;
}

.coupons .quota {
  font-size: 36rpx;
  font-weight: 700;
}

.coupons .requirement {
  display: block;
  font-size: 22rpx;
  color: #e05d32;
}

/*实付金额*/

.pay-amount {
  width: 100%;
  height: 100rpx;
  margin-top: 1px;
  text-align: right;
  color: #353535;
  background: #fff;
  line-height: 100rpx;
}

.pay-amount .pay-fee {
  color: #e4383b;
  font-size: 32rpx;
  padding-left: 10rpx;
}

.pay-amount .realfee {
  color: #e4383b;
  font-weight: 700;
  font-size: 42rpx;
  margin-right: 20rpx
}
.pay-amount .huise,
.pay-amount .huise .realfee{
  font-size: 32rpx;
  color: #666;
  text-decoration: line-through;
}
.pay-amount text {
  font-size: 34rpx;
  margin-right: 5rpx;
}

/*支付按钮*/

.pay-btn {
  width: 92%;
  height: 90rpx;
  background: #390;
  line-height: 90rpx;
  margin-top: 40rpx;
  color: white;
  text-align: center;
  font-weight: 600;
  font-size: 36rpx;
  margin-bottom: 30rpx;
}

.refer-btn {
  width: 92%;
  height: 85rpx;
  background: #ff5319;
  line-height: 85rpx;
  margin-top: 40rpx;
  color: white;
  text-align: center;
  font-weight: 600;
  font-size: 36rpx;
  margin-bottom: 30rpx;
}

.kong {
  padding-left: 30rpx;
}


/*优惠码*/
.promo-box{
  position: relative
}
.model-box{
  color: #333;
}
.promo{
  padding: 15rpx;
  border:1rpx solid #ddd;
  border-radius: 10rpx;
}
.promo-hint{
  padding-top: 36rpx;
  color: red;
}
.promo-box .coupons{
  text-align: right;
  margin-right: 70rpx;
  width: 450rpx;
}
.coupons .yhm-text{
  color: #949494;
  font-size: 30rpx;
  font-weight:normal;
}
.jiantou{
  width:45rpx;
  height:45rpx;
  position:absolute;
  top: 30rpx;
  right: 35rpx;
  background:url('data:image/png;base64,') no-repeat center center;
  background-size:100% 100%;
}
.scroll-coupons{
  width: 100%;
height: 100rpx;
overflow: hidden;
white-space: nowrap;
}