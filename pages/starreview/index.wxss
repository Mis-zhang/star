/* pages/review/index.wxss *//* pages/publish/index.wxss */
.publish {
  width: 100%;
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding-bottom: 110rpx;
  z-index: 666;
}
.publish_height {
  height: 100%;
}
.publish .publish_top {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding-bottom: 50rpx;
  background: #ffffff;
}
.publish .publish_top .publish_goods {
  width: 100%;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  border-bottom: 1px solid #eeeeee;
}
.publish .publish_top .publish_goods .publish_title {
  height: 80rpx;
  display: flex;
  align-items: center;
  font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
  color: #333333;
  padding-left: 35rpx;
}
.publish .publish_top .publish_goods .publish_breed {
  height: 80rpx;
  display: flex;
  align-items: center;
  font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
  color: #333333;
  padding-left: 18rpx;
}
.publish .publish_top .publish_goods .publish_norm {
  height: 80rpx;
  display: flex;
  align-items: center;
  font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
  color: #aaaaaa;
  padding-left: 30rpx;
}
.publish .publish_top .publish_content {
  width: 100%;
  /* padding: 30rpx 0rpx; */
  display: flex;
  flex-direction: column;
  align-items: center;
}
.publish_content .publish_star {
  width: 700rpx;
  height: 35rpx;
  padding: 0rpx 25rpx;
  display: flex;
  flex-direction: row;
}
.publish_content .publish_star .publish_star_title {
  height: 35rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
  color: #333333;
  padding-left: 10rpx;
}
.publish_content .publish_star .publish_star_num {
  height: 35rpx;
  width: 195rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  padding-left: 32rpx;
}
.publish_content .publish_star .publish_star_num .publish_star_icon {
  width: 35rpx;
  height: 35rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-image: url('data:image/png;base64,');
  background-size: cover;
  background-repeat: no-repeat;
}
.publish_content .publish_star .publish_star_num .publish_star_icon_active {
  width: 35rpx;
  height: 35rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-image: url('data:image/png;base64,');
  background-size: cover;
  background-repeat: no-repeat;
}
.publish_content .publish_star .publish_star_tips {
  height: 35rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  padding-left: 27rpx;
  font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	color: #aaaaaa;
}
.publish_content .publish_write {
  width: 650rpx;
  padding: 30rpx 25rpx;
  background: #f5f5f5;
  border-radius: 6rpx;
  margin-top: 41rpx;
}
.publish_content .publish_write .publish_write_text {
  width: 100%;
}
textarea {
  width: 100%;
  font-size: 28rpx;
  color: #333333;
  min-height: 100rpx;
}
.texterea {
  color: #aaaaaa;
  font-size: 28rpx;
}
.publish_content .publish_write .publish_write_creame {
  width:100%;
  margin-top: 40rpx;
  display: flex;
  flex-direction: column;
}
.publish_write_creame .add {
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.publish_write_creame .add .add_icon {
  width: 140rpx;
  height: 140rpx;
  margin-right: 28rpx;
  margin-bottom: 20rpx;
  position: relative;
  background: #ffffff;
}
.publish_write_creame .add .add_icon:nth-child(4),
.publish_write_creame .add .add_icon:nth-child(8)
 {
  margin-right: 0rpx;
}
.publish_write_creame .add .add_icon .add_icon_img {
  width: 140rpx;
  height: 140rpx;
}
.add_icon_active {
  position: absolute;
  right: -10rpx;
  top: -10rpx;
  width: 38rpx;
  height: 38rpx;
  z-index: 9999;
  /* background: #ffffff; */
}
 .photo {
  width: 137rpx !important;
  height: 137rpx !important;
  border: 1rpx dashed #dddddd;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
 .photo .photo_icon {
  width: 62rpx;
  height: 53rpx;
}
.photo .photo_icon .photo_icon_img {
  width: 62rpx;
  height: 53rpx;
}
 .photo .photo_title {
  width: 100%;
  font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
  color: #aaaaaa;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 20rpx;
}
.submit {
  width: 100%;
  height: 110rpx;
  font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
  color: #ffffff;
  background: #e03836;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 9999;
}