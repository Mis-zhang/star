.searchList{
  width: 100%;
  height: 100%;
  box-sizing: border-box;
}
.srarchTitle{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  margin-bottom: 20rpx;
  background-color: #fff;
  border-bottom: 1px solid #eee;
  text-align: center;
  color: #c2c2c2;
  font-size: 32rpx;
}

.shop_lists{
  width: 92%;
  padding: 0 4%;
  background: #fff;
  display: flex;
  flex-direction:row;
 flex-wrap:wrap;
 justify-content:space-between;
}
.lists_every{
  width: 334rpx;
  height: 466rpx;
  margin-bottom: 20rpx;
}
.everyImg{
  width: 100%;
  height: 334rpx;
  box-sizing: border-box;
  border: 1px solid #eaeaea;
}
.everyImg image{
  width: 100%;
  height: 100%;
}
.shop_content{
  height: 132rpx;
  width: 100%;
}
.shopName{
  display: inline-block;
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  font-size: 34rpx;
  color: #000;
  overflow: hidden; 
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-top: 10rpx;
}
.shop_content .price{
  display: block;
  text-align: center;
  font-size: 34rpx;
  font-weight: 700;
  color: #e4383b;
}
.shop_content .priceSmall{
  font-size: 24rpx;
}
.showTile{
  width: 100%;
  height: 200rpx;
  line-height: 200rpx;
  text-align: center;
  color: #999;
  font-size: 40rpx;
}
.load {
  width: 100%;
  height: 150rpx;
  background-color: #fff;
  line-height: 150rpx;
  text-align: center;
  position: relative;
}

.load .change {
  width: 100rpx;
  height: 20rpx;
  position: absolute;
  top: 70rpx;
  right: 200rpx;
}

.load .change view {
  width: 10rpx;
  height: 10rpx;
  border-radius: 50% 50%;
  background-color: #fff;
  border: 1rpx solid #333;
  float: left;
  margin-left: 15rpx;
}

.load .loadstyle {
  width: 275rpx;
  height: auto;
  margin: 0 auto;
}

.load .change .on {
  background: #333;
}

.load .loadover {
  color: #333;
}

.load .loadover view {
  width: 150rpx;
  height: 1rpx;
  background: #333;
}

.load .loadover .left {
  position: absolute;
  top: 75rpx;
  left: 125rpx;
}

.load .loadover .right {
  position: absolute;
  top: 75rpx;
  right: 125rpx;
}

.load .change .on {
  background: #333;
}
.shopnone {
  width: 100%;
  height: 150rpx;
  line-height: 150rpx; 
  text-align: center;
  color: #999;
  font-size: 38rpx;
}
.load .change .on {
  background: #333;
}