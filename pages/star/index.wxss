/* pages/star/index.wxss */
.star {
  width: 100%;
  /* height: 100%; */
  display: flex;
  flex-direction: column;
  align-items: center;
  background: #ffffff;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 8999;        
}
.star .star_tab {
  width: 100%;
  height: 100rpx;
  border-bottom: 1rpx solid #eeeeee;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-around;
  align-items: center;
  /* position: fixed; */
  /* top: 0; */
  /* left: 0; */
  /* z-index: 9999; */
}
.star .star_tab .tab_item {
  height: 80rpx;
  height: 80rpx;
  padding: 10rpx 0rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  width: 100%;
}
.star .star_tab .tab_item .item_title {
  width: 100%;
  font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
  color: #333333;
  display: flex;
  justify-content: center;
  align-items: center;
}
.star .star_tab .tab_item .item_num {
  width: 100%;
  font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
  color: #aaaaaa;
  display: flex;
  justify-content: center;
  align-items: center;
}
.star .star_tab .tab_item.tab_active {
  border-bottom: 1px solid #dd3841;
}
.star .star_tab .tab_item .item_title.tab_item_active {
  color: #dd3841;
}
.star .star_tab .tab_item .item_num.tab_item_active {
  color: #dd3841;
}
