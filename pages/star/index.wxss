.star_scroll {
  width: 710rpx;
  display: flex;
  flex-direction: column;
}
.star_scroll .star_list {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.star_scroll .star_list .star_item {
  width: 670rpx;
  padding: 34rpx 20rpx;
  border-bottom: 1rpx solid #eeeeee;
}
.star_item .star_top {
  width: 100%;
  height: 30rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
}
.star_top .star_top_left {
  height: 30rpx;
  display: flex;
  flex-direction: row;
}
.star_top .star_top_left .star_tel {
  height: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	color: #333333;
}
.star_top .star_top_left .star_star {
  width: 195rpx;
  height: 30rpx;
  margin-left: 33rpx;
  display: flex;
  flex-direction: row;
  /* justify-content: space-between; */
}
.star_top .star_top_left .star_star .star_icon {
  width: 30rpx;
  height: 30rpx;
  margin-right: 8rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.star_top .star_top_left .star_star .star_icon .star_icon_img {
  width: 30rpx;
  height: 30rpx;
}
.star_top .star_time {
  height: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	color: #aaaaaa;
}
.star_item .star_container {
  width: 100%;
  margin-top: 38rpx;
  font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 42rpx;
	letter-spacing: 2rpx;
  color: #333333;
  margin-bottom: 22rpx;
}
.star_item .star_update {
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  /* display: none; */
}
.star_item .star_update .star_update_img {
  width: 150rpx;
  height: 150rpx;
  margin-right: 23rpx;
  margin-bottom: 20rpx;
}
.star_item .star_update .star_update_img:nth-child(4) {
    margin-right: 0rpx;
}
.star_item .star_update .star_update_img:nth-child(8) {
  margin-right: 0rpx;
}
.star_item .star_norm {
  width: 100%;
  font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	color: #aaaaaa;
}
.star_item .star_reply {
  width: 610rpx;
  padding: 23rpx 44rpx 33rpx 26rpx;
  margin-top: 20rpx;
  background: #f5f5f5;
  border-radius: 3rpx;
  display: flex;
  flex-direction: column;
}
.star_item .star_reply .star_reply_top {
  width: 100%;
  height: 50rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.star_item .star_reply .star_reply_top .star_reply_icon {
  width: 50rpx;
  height: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.star_item .star_reply .star_reply_top .star_reply_icon .star_reply_img {
  width: 28rpx;
  height: 28rpx;
}
.star_item .star_reply .star_reply_top .star_reply_title {
  height: 50rpx;
  font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
  color: #666666;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 10rpx;
}
.star_item .star_reply .star_reply_content {
  width: 100%;
  margin-top: 20rpx;
  font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 42rpx;
	letter-spacing: 2rpx;
	color: #333333;
  word-break: break-all;
}
.star_item .star_review {
  width: 100%;
  display: flex;
  flex-direction: column;
}
.star_item .star_review .star_review_title {
  height: 30rpx;
  display: flex;
  /* justify-content: center; */
  align-items: center;
  font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
  color: #dd3841;
  margin-top: 20rpx;
}
.star_item .star_review .star_review_container {
  width: 100%;
  margin-top: 10rpx;
  font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 42rpx;
	letter-spacing: 2rpx;
  color: #333333;
  margin-bottom: 22rpx;
}
.star_item .star_review .star_review_update {
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.star_item .star_review .star_review_update .star_update_img {
  width: 150rpx;
  height: 150rpx;
  margin-right: 23rpx;
  margin-bottom: 20rpx;
}
.star_item .star_review .star_review_update .star_update_img:nth-child(4) {
    margin-right: 0rpx;
}
.star_item .star_review .star_review_update .star_update_img:nth-child(8) {
  margin-right: 0rpx;
}
.nostar {
  display:flex;
justify-content:center;
align-items:center;
width:100%;
height:100%;
color:#999999;
font-size:30rpx;
position: absolute;
left: 50%;
top: 50%;
transform: translate(-50%, -50%);
z-index: 2;
}
.isstar {
  display: none;
}
/*  加载更多   */
.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.weui-loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  color: #666666;
  text-align: center;
}
.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}
