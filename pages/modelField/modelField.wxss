/* pages/modelField/modelField.wxss */
/* 搜索框 */
page{
  height: 100%;
}
.container{
   width: 100%;
    height: 100%;   
   box-sizing: border-box; 
   background-color: #f5f5f5;
}
.search{
  width: 100%;
  height: 10%;  
  padding: 18rpx 30rpx;
  overflow: hidden;
  background: #fff;
  position: fixed;
  top: 0;
  left: 0;
  box-sizing: border-box; 
  background-color: #22292c;
  z-index: 1000;
}
.search input{
   padding-left: 20rpx;
   width: 100%;
   height: 80rpx;
   line-height: 80rpx;
   font-size: 30rpx;
   color: #333;
   float: left;
   border:1px solid #f5f5f5;
   border-right:0;
   box-sizing:border-box;
   background-color: #fff;
   border-radius: 40rpx;
}
.search button{
  width: 140rpx;
  height: 80rpx;
  line-height: 80rpx;
  background: #eeeeee;
  color: #303030;
  float: left;
  border-radius: 0;
  font-size: 34rpx;
}
.search .search_ifier{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 38rpx;
  right: 60rpx;
  z-index: 999;
  background: url('data:image/png;base64,') no-repeat center center;
  background-size: contain;
}
.classification{
  width: 100%;
  min-height: 100%; 
  margin-top: 13%;
  border-top: 8rpx solid #eee;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
}
.classification_left{
  width: 25%;
  height: 90%;
  background: #fff;
  position: fixed;
  left: 0;
  top: 10%;
  background-color: #f7f7f7;
}
.shop_right{
  margin-left: 25%;
  width: 75%;
  min-height: 100%;
  background-color: #fff;
}
.classification_top{
  width: 100%;
  height: auto;
  margin-bottom: 110rpx;
}
.classification_title{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
}
.classification_title text{
  display: inline-block;
  width: 100%;
  font-size: 28rpx;
  text-align: center;
  overflow: hidden;
}
.shop_top{
  padding: 0 10rpx;
  margin-top: 56rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap; 
  justify-content: space-between;
  box-sizing: border-box;
}
.border_color{
  border-left: 6rpx solid #390;
  background-color: #fff;
}
.shop_content{
  width: 48%;
  margin-bottom: 14rpx;
}
.shopImg{
  width: 100%;
  height: 238rpx; 
  border: 1px solid #eaeaea;
  box-sizing: border-box;
  overflow: hidden;
}
.shopImg image{
  width: 100%;
  height: 238rpx;
}
.shopExhibition{
  padding-top: 10rpx;
  width: 100%;
  height: 98rpx;
}
.shopName{
  display: inline-block;
  width: 100%;
  text-align: center;
  font-size: 30rpx;
  overflow: hidden; 
  text-overflow: ellipsis;
  white-space: nowrap;
}
.price{
  display: inline-block;
  width: 100%;
  text-align: center;
  font-size:34rpx;
  font-weight: 700;
  color:#e4383b;
}
.priceSmall{
  font-size: 24rpx;
}
.load {
  width: 100%;
  height: 150rpx;
  background-color: #fff;
  line-height: 150rpx;
  text-align: center;
  position: relative;
}

.load .change {
  width: 100rpx;
  height: 20rpx;
  position: absolute;
  top: 70rpx;
  right: 120rpx;
}

.load .change view {
  width: 10rpx;
  height: 10rpx;
  border-radius: 50% 50%;
  background-color: #fff;
  border: 1rpx solid #333;
  float: left;
  margin-left: 15rpx;
}

.load .loadstyle {
  width: 275rpx;
  height: auto;
  margin: 0 auto;
}

.load .change .on {
  background: #333;
}

.load .loadover {
  color: #999;
}

.load .loadover view {
  width: 100rpx;
  height: 3rpx;
  background: #999;
}

.load .loadover .left {
  position: absolute;
  top: 75rpx;
  left: 90rpx;
}

.load .loadover .right {
  position: absolute;
  top: 75rpx;
  right: 90rpx;
}

.load .change .on {
  background: #333;
}
.shopnone {
  width: 100%;
  height: 150rpx;
  line-height: 150rpx; 
  text-align: center;
  color: #aaa;
  font-size: 32rpx;
}

