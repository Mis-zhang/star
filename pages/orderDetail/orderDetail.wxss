/* pages/orderDetail/orderDetail.wxss */

/*商家联系电话*/

.telephone {
  width: 100%;
  height: 190rpx;
  background: #ff5319;
}
.shopqty{
  padding-left:30rpx;
}
.telephone .tel-logo {
  width: 17%;
  height: 100%;
  background: url('data:image/png;base64,') no-repeat 10rpx 50rpx;
  background-size: 100rpx;
  margin-left: 3.33%;
  float: left;
}

.telephone .tel-content {
  width: 77.66%;
  height: auto;
  margin-left: 1.66%;
  float: left;
  font-size: 30rpx;
  color: #fff;
  margin-top: 60rpx;
}

.telephone .tel-content .contact {
  width: auto;
  height: 50rpx;
  font-weight: 600;
  font-size: 27rpx;
}

.telephone .tel-content .traderName {
  width: auto;
  height: 30rpx;
  margin-left: 6rpx;
  font-weight: 600;
  font-size: 32rpx;
}

.telephone .tel-content .traderMob {
  width: auto;
  height: auto;
  padding-top: 15rpx;
  font-size: 32rpx;
  margin-left: 25rpx;
  font-weight: 600;
}

/* 具体收货地址*/

.specific-addr {
  overflow: hidden;
  width: 100%;
  /* height: 140rpx; */
   padding-bottom: 10rpx;  
  margin-top: 1rpx;
  background: #fff;
}

.specific-addr .add-logo {
  width: 8%;
  /* height: 100%; */
  height: 105rpx;
  background: url('data:image/png;base64,') no-repeat 0rpx 30rpx;
  background-size: 50rpx 57rpx;
  margin-left: 3.33%;
  float: left;
}

.specific-addr .receiver-content {
  width: 450rpx;
  height: auto;
  margin-left: 4.66%;
  float: left;
  font-size: 30rpx;
  color: #4b4b4b;
  margin-top: 30rpx;
}

.specific-addr .receiver-content .contactName {
  width: auto;
  height: 30rpx;
  color: #353535;
  font-weight: 600;
  font-size: 32rpx;
}

.specific-addr .receiver-content .contactMob {
  /* width: auto; */
  /* width: 100%; */
  /* height: 30rpx; */
  color: #353535;
  margin-left: 30rpx;
  font-weight: 600;
  font-size: 32rpx;
   word-break:break-all;
   word-wrap:break-word;
}

.specific-addr .receiver-content .contactAddr {
  display: block;
  width: auto;
  height: auto;
  color: #353535;
  padding-top: 15rpx;
  font-size: 25rpx;
}

/*订单详情*/

.shop-detail {
  width: 100%;
  margin-top: 20rpx;
  background-color: #fff;
  padding: 30rpx 0 30rpx 30rpx;
  position: relative;
  overflow: hidden;
  box-sizing: border-box;
  display: flex;
}

.shop-detail image {
  float: left;
  width: 170rpx;
  height: 170rpx;
  border-bottom: 1px solid #fef5f5;
}

.shop-detail .company-name {
  display: block;
  font-size: 30rpx;
  color: #c3c3c3;
  line-height: 1;
  overflow: hidden;
  margin-top: 10rpx;
  height: 61rpx;
}

.info-wrap {
  padding: 0 30rpx;
  width: 380rpx;
  overflow: hidden;
  flex: 1;
}

.info-wrap .tit {
  font-size: 30rpx;
  color: #000;
  line-height: 40rpx;
  overflow: hidden;
}

.price-info {
  padding: 10rpx 0 0rpx;
  overflow: hidden;
}

.price-info text {
  font-size: 26rpx;
  color: #b2b2b2;
  float: left;
}

.price-info .price {
  font-size: 35rpx;
  color: #e4383b;
  float: right;
}

/*商品的总额和运费*/

.coast {
  width: 100%;
  height: auto;
  margin-top: 20rpx;
  color: #b2b2b2;
  background: #fff;
  display: flex;
  flex-direction: column;
}

.coast .total-fee, .freight, .full-cut, .full-folding,.full-yhm,.integral {
  flex: 1;
  margin: 15rpx 35rpx;
  font-size: 30rpx;
}

.coast .shop-coast, .freight-txt, .cut-txt, .folding-txt,.integral-txt,.yhm-text {
  float: left;
}

.coast .shop-fee, .freight-fee, .cut-fee, .folding-fee ,.integral-fee,.yhm-fee{
  float: right;
}
.yhm-fee{
  color: #b2b2b2;
}



/*商品的总额+运费*/

.total {
  width: 100%;
  height: 100rpx;
  background: #fff;
  line-height: 100rpx;
  font-size: 35rpx;
  margin-top: 1px;
}

.total .total-txt {
  float: left;
  /* font-weight: 600; */
  color: #353535;
  margin-left: 35rpx;
}

.total .total-fee {
  float: right;
  font-weight: 600;
  color: #e4383b;
  margin-right: 35rpx;
}

/*支付方式*/

.pay-way {
  width: 100%;
  height: 100rpx;
  background: #fff;
  line-height: 100rpx;
  margin: 20rpx 0rpx;
  font-size: 35rpx;
}

.pay-way .pay-txt {
  float: left;
  /* font-weight: 600; */
  color: #353535;
  margin-left: 35rpx;
}

.pay-way .pay-wx {
  float: right;
  /* font-weight: 600; */
  color: #666;
  margin-right: 35rpx;
}

/*订单编号*/

.order-card {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
  font-size: 35rpx;
}

.order-card .card-txt {
  float: left;
  /* font-weight: 600; */
  color: #353535;
  margin-left: 35rpx;
}

.order-card .card-id {
  float: right;
  /* font-weight: 600; */
  color: #666;
  margin-right: 35rpx;
}

/*商品满赠的物品的名称，规格，和数量*/

.full-gifts {
  width: 100%;
  height: auto;
  margin-top: 20rpx;
  background: #fff;
  color: #b2b2b2;
  font-size: 30rpx;
  text-align: right;
  line-height: 100rpx;
}

.giftdetail {
  width: 650rpx;
  height: auto;
  display: flex;
  flex-direction: column;
  background-color: #fff;
  margin-left: 100rpx;
}

.zeng {
  padding-left: 35rpx;
  float: left;
}

.giftData {
  border-bottom: 1px solid #f5f5f5;
  flex: 1;
}

.giftname {
  float: left;
}

.giftnumber {
  float: right;
  padding-right: 40rpx;
}

/*下单时间*/

.order-time {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
  font-size: 35rpx;
  margin-top: 1px;
  margin-bottom:40rpx;
}

.order-time .time-txt {
  float: left;
  /* font-weight: 600; */
  color: #353535;
  margin-left: 35rpx;
}

.order-time .time-id {
  float: right;
  /* font-weight: 600; */
  color: #666;
  margin-right: 35rpx;
}

/*选择按钮*/

.pay-btn {
  width: 92%;
  height: 85rpx;
  background: #e4383b;
  line-height: 85rpx;
  color: white;
  text-align: center;
  font-weight: 600;
  font-size: 36rpx;
  margin-bottom: 30rpx;
}

.refer-btn {
  width: 92%;
  height: 85rpx;
  background: #fff;
  line-height: 85rpx;
  margin-top: 20rpx;
  color: #666;
  text-align: center;
  font-weight: 600;
  font-size: 36rpx;
  margin-bottom: 30rpx;
}
.tuibox{
  width: 100%;
  background: rgba(0, 0, 0, .5);
  position: absolute;
  top:0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 100000
}
.tuikuan{
  width: 100%;
  height: 580rpx;
  position: fixed;
  top: 50%;
  margin-top: 30rpx;
  z-index: 99;
  font-size: 28rpx;
  background: #fff;
  box-sizing: border-box;
  padding: 10rpx 20rpx;
  box-shadow: 0,0,0,rgba(0, 0, 0, .5)
}
.checkbox{
  width: 100%;
  display: block;
  /* text-align: center; */
  margin-top: 30rpx;
}
.checkbtn{
  width: 100%;
}
.zhezhao{
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .5);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 98;
}
.tuiButton{
  display: inline-block;
  width: 48%;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  margin-top: 30rpx;
  font-size: 26rpx;
  color: #fff;
}
.tuiButton:first-child{
  margin-right: 2%
}
.tuiButton:last-child{
  margin-left: 2%
}

.left{
  background: #ec9d2b;
}
.right{
  background: #390;
}
