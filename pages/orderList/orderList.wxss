.checkbox{
  width: 100%;
  display: block;
  /* text-align: center; */
  margin-top: 30rpx;
}
.checkbtn{
  width: 100%;
}
.zhezhao{
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .5);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 98;
}
.tuiButton{
  display: inline-block;
  width: 48%;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  margin-top: 30rpx;
  font-size: 26rpx;
  color: #fff;
}
.tuiButton:first-child{
  margin-right: 2%
}
.tuiButton:last-child{
  margin-left: 2%
}

.left{
  background: #ec9d2b;
}
.right{
  background: #390;
}
.order-pay-cancel {
  color:　#ccc;
  font-size: 28rpx;
}
.isDisplay{
