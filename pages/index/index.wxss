/* pages/index/index.wxss */
/* 公告 */
.remind{
  width: 42rpx;
  height:42rpx;
}
.notice-text {
  width: 20%;
  text-align: center;
  font-size: 28rpx;
}
.example {
  display: flex;
  justify-content: space-between;
  padding: 10rpx;
  /* width: 100%; */
  background-color: #fff;
  color: #666;
}
.marquee_box {
  width: 80%;
  position: relative;
  overflow: hidden;
}
.marquee_text {
  white-space: nowrap;
  position: absolute;
  top: 1rpx;
  font-size: 28rpx;
}
/* 轮播 */

.banner {
  width: 100%;
  height: 420rpx;
  position: relative;
}
.mask{
  position:fixed;
  top:0rpx;
  left:0rpx;
  z-index:200;
}
.banner image {
  width: 100%;
  height: 100%;
}

/* banner图dots*/

.banner .wx-swiper-dots.wx-swiper-dots-horizontal {
  margin-bottom: 20rpx;
}

.banner .wx-swiper-dot {
  display: inline-flex;
  width: 20rpx;
  height: 20rpx;
  margin-left: 40rpx;
  border-radius:50% 50%;
  z-index: 50;
  background: #fff;
  border: 1px solid #2a415c;
}

.banner .wx-swiper-dot-active {
  background: #2a415c;
}

.shopnone {
  width: 100%;
  height: 150rpx;
  line-height: 150rpx; 
  text-align: center;
  color: #999;
  font-size: 38rpx;
}

/* 优惠卷 start */
.coupon-choose{
  padding: 30rpx 20rpx 0 20rpx;
  width: 100%;
  height: 200rpx;
  background: #fff;
  box-sizing: border-box;
}
.coupons-con {
  width: 100%;
  float: left;
  height: 100%;
}
.block{
  position: absolute;
  top: 0rpx;
  left: 0rpx;
}
.coupons{
  float: left;
  margin-right: 40rpx;
  width: 400rpx;
  height: 140rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: contain;
}
.coupons:last-child{
  margin-right: 0rpx;
}
.couponPrice{
  margin-top: 30rpx;
  width: 160rpx;
  color: #fff;
  text-align: center;
  float: left;
}
.couponPrice-right{
  float: left;
  width: 220rpx;
}
.yhqname{
 padding-top: 20rpx;
  color: #d14e4e;
}
.price_sign{
  font-size: 24rpx;
}
.priceShu{
  font-size: 58rpx;
}
.receiveConditions{
  display: inline-block;
  width: 200rpx;
  font-size: 24rpx;
  color: #fde9e9;
}
/* 优惠卷 end */

/* 搜索框 start*/
.search{

  width: 100%;
  height: 116rpx;
  background: #fff;
  position: relative;
  overflow: hidden;
  box-sizing: border-box;
  background-color: #22292c;
}
.search input{
   margin: 0 3%;
   padding-left: 2%;
   width: 94%;
   height: 80rpx;
   margin-top: 18rpx;
   line-height: 80rpx;
   font-size: 30rpx;
   color: #333;
   float: left;
   border:1px solid #f5f5f5;
   box-sizing:border-box;
   background-color: #fff;
   border-radius: 40rpx;
}
.search-button{
  margin-top: 18rpx;
  width:140rpx;
  height:80rpx;
  line-height: 80rpx;
  text-align: center;
  background-color:#eee;
  font-size: 32rpx;
  color: #444;
  float: left;
  border-radius: 0;
}
.search .search_ifier{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 38rpx;
  right: 60rpx;
  z-index: 999;
  background: url('data:image/png;base64,') no-repeat center center;
  background-size: contain;
}
/* 搜索框 end*/

/* 商品列表 新 start */
.shop_min{
  margin-top: 20rpx;
  padding: 0 20rpx;
  background: #fff;
  box-sizing: border-box;
}
.recommend{
  width: 100%;
  height: 124rpx;
  text-align: center;
  line-height: 124rpx;
}
.shop_lists{
  width: 100%;
  background: #fff;
  display: flex;
  flex-direction:row;
 flex-wrap:wrap;
 justify-content:space-between;
 
}
.lists_every{
  width: 334rpx;
  height: 466rpx;
  margin-bottom: 20rpx;
}
.everyImg{
  width: 100%;
  height: 334rpx;
  box-sizing: border-box;
  border: 1px solid #eaeaea;
}
.everyImg image{
  width: 100%;
  height: 100%;
}
.shop_content{
  height: 132rpx;
  width: 100%;
}
.shopName{
  display: inline-block;
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 34rpx;
  color: #000;
  overflow: hidden; 
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: center;
  margin-top: 10rpx;
}
.shop_content .price{
  display: block;
  font-size: 34rpx;
  font-weight: 700;
  color: #e4383b;
  text-align: center;
}
.shop_content .priceSmall{
  font-size: 24rpx;
}

/* 商品列表 新 end */


/*没有授权*/

.noPosition {
  padding: 30rpx;
  padding-top: 600rpx;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
  margin: auto;
  background: url(data:image/png;base64,) no-repeat center 200rpx;
  background-size: 245rpx 245rpx;
  text-align: center;
  overflow: hidden;
}

.noPosition text {
  display: inline-block;
  line-height: 80rpx;
  overflow: hidden;
  font-size: 32rpx;
  margin-right: 20rpx;
  color: #333;
}

.noPosition .button {
  display: inline-block;
  color: #fff;
  overflow: hidden;
  font-size: 32rpx;
  background: linear-gradient(#38ae3b, #226d25);
  box-shadow: 2rpx 2rpx 6rpx #154216;
  width: 170rpx;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 10rpx 10rpx;
}

/*加载中*/

.load {
  width: 100%;
  height: 150rpx;
  background-color: #f5f5f5;
  line-height: 150rpx;
  text-align: center;
  position: relative;
}

.load .change {
  width: 100rpx;
  height: 20rpx;
  position: absolute;
  top: 70rpx;
  right: 200rpx;
}

.load .change view {
  width: 10rpx;
  height: 10rpx;
  border-radius: 50% 50%;
  background-color: #fff;
  border: 1rpx solid #333;
  float: left;
  margin-left: 15rpx;
}

.load .loadstyle {
  width: 275rpx;
  height: auto;
  margin: 0 auto;
}

.load .change .on {
  background: #333;
}

.load .loadover {
  color: #aaa;
}

.load .loadover view {
  width: 150rpx;
  height: 1rpx;
  background: #aaa;
}

.load .loadover .left {
  position: absolute;
  top: 75rpx;
  left: 125rpx;
}

.load .loadover .right {
  position: absolute;
  top: 75rpx;
  right: 125rpx;
}
/* --tab-swich-start */
.tab-swich {
  background-color: #fff;
  display: flex;
  font-size: 28rpx;
  color: #666;
  text-align: center;
  margin-top: 20rpx;
}
.tab-swich text {
  flex: 1;
  line-height: 100rpx;
  border-bottom: 1rpx solid #999;
  transition: all .5s;
}
.tab-swich .on {
  border-bottom-color: #f60;
  font-size: 32rpx;
  color: #f60;
}

/* picker  Start */
.crop-wrap {
  margin: 20rpx 30rpx;
  border: 1rpx solid #ddd;
  border-radius: 6rpx;
  line-height: 75rpx;
  text-align: center;
  height: 75rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
}
/* picker  END */